{
  "name": "qbun",
  "module": "index.ts",
  "private": true,
  "version": "0.0.1",
  "description": "A simple Bun CLI tool",
  "bin": {
    "qbun": "src/index.ts"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=node",
    "dev": "bun run src/index.ts",
    "fmt": "oxfmt",
    "lint": "oxlint",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^14.0.2"
  }
}