{
  "name": "@qmaru/qbun",
  "version": "1.0.0",
  "description": "A simple Bun CLI tool",
  "bin": {
    "qbun": "dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=node",
    "postbuild": "cp node_modules/@qmaru/rtools-web/rtools_web_bg.wasm dist/",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "fmt": "oxfmt",
    "lint": "oxlint"
  },
  "files": [
    "dist"
  ],
  "author": "qmaru",
  "license": "ISC",
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0"
  },
  "dependencies": {
    "@qmaru/mdp-js": "^1.1.4",
    "@qmaru/rtools-web": "^1.3.4",
    "commander": "^14.0.2"
  },
  "publishConfig": {
    "@qmaru:registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}