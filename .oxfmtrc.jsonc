{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "trailingComma": "all",
    "proseWrap": "preserve",
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf"
}